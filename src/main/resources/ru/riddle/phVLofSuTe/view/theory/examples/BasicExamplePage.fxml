<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_CENTER" type="VBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="title" alignment="CENTER" contentDisplay="CENTER" maxWidth="745.0" prefWidth="1000000.0" text="Title">
      <font>
         <Font name="Calibri Bold" size="36.0" />
      </font>
      <padding>
         <Insets bottom="10.0" left="120.0" right="100.0" top="10.0" />
      </padding>
      <VBox.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </VBox.margin>
   </Label>
   <ListView fx:id="textList" styleClass="theory-examples-text-list" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets left="20.0" right="20.0" />
      </VBox.margin></ListView>
</fx:root>
